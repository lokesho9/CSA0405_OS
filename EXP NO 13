#include <stdio.h>
int main(){
    int nb,np,b[10],p[10],alloc[10],i,j;
    printf("Enter blocks: "); scanf("%d",&nb);
    for(i=0;i<nb;i++) scanf("%d",&b[i]);
    printf("Enter processes: "); scanf("%d",&np);
    for(i=0;i<np;i++){ scanf("%d",&p[i]); alloc[i]=-1; }
    for(i=0;i<np;i++) for(j=0;j<nb;j++)
        if(b[j]>=p[i]){ alloc[i]=j; b[j]-=p[i]; break; }
    for(i=0;i<np;i++)
        if(alloc[i]!=-1) printf("P%d -> B%d\n",i+1,alloc[i]+1);
        else printf("P%d -> Not Allocated\n",i+1);
}
OUPUT:
Enter blocks: 4
1
2
3
4
Enter processes: 4
5
6
7
8
P1 -> Not Allocated
P2 -> Not Allocated
P3 -> Not Allocated
P4 -> Not Allocated
