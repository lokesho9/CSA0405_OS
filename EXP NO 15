#include <stdio.h>
int main(){
    int u,f,i,j; char fname[10][10][20];
    printf("Enter users: "); scanf("%d",&u);
    for(i=0;i<u;i++){
        printf("Files for User%d: ",i+1); scanf("%d",&f);
        for(j=0;j<f;j++) scanf("%s",fname[i][j]);
    }
    for(i=0;i<u;i++){
        printf("User%d:\n",i+1);
        for(j=0;j<f;j++) printf("  %s\n",fname[i][j]);
    }
}
OUPUT:
Enter files: 2
1
2
Directory:
1
2
